<p>
  <strong>Name:</strong>
  <%= @project.name %>
</p>

<p>
  <strong>Description:</strong>
  <%= @project.description %>
</p>


<div id="tasks_wrapper">

    <%= render template: "tasks/index", locals: { tasks: @tasks, completed: @completed } %>


  <div id="form">
    <%= render "tasks/form", task: @project.tasks.build %>
  </div>
</div>

<%= link_to 'Edit', edit_project_path(@project) %> |
<%= link_to 'Back', projects_path %>
